{
    "config": {
        "BackupDestination": "/path/to/backup/destination"
    },
    "telegram": {
        "ChatID": "your-chat-id",
        "APIkey": "your-api-key",
        "MessageThreadID": "123"
    },
    "stacks": [
        {
            "name": "stack1",
            "composeFile": "/path/to/compose.yml",
            "volumes": [
                {
                    "path": "/path/to/volume1",
                    "maxBackups": 5,
                    "preBackupSleep": 10
                },
                {
                    "path": "/path/to/volume2",
                    "maxBackups": 3
                }
            ]
        },
        {
            "name": "stack2",
            "composeFile": "/path/to/compose2.yml",
            "volumes": [
                {
                    "path": "/path/to/volume3",
                    "maxBackups": 7
                }
            ]
        }
    ]
}